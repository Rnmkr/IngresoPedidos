        xmlns:local="clr-namespace:IngresoPedidos.Views"
        mc:Ignorable="d"
        Title="EDITAR PEDIDO 1234567A-00" Height="172" Width="720" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="10">
        <Label Content="PEDIDO" Margin="4,0,0,38" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="100" Height="15" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Label Content="ARTICULO" Margin="340,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="63" HorizontalAlignment="Left"/>
        <Label Content="MODELO" Margin="105,3,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="115" HorizontalAlignment="Left"/>
        <Label Content="PRODUCTO" Margin="220,3,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="115" HorizontalAlignment="Left"/>
        <Label Content="CANTIDAD" Margin="408,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="54" HorizontalAlignment="Left"/>
        <Label Content="ORIGINAL" Margin="588,0,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="96" HorizontalAlignment="Left"/>
        <Label Content="ESTADO" Margin="473,3,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="110" HorizontalAlignment="Left"/>
        <TextBox Height="26" TextWrapping="Wrap" Text="1234567A-00" VerticalAlignment="Top" Width="96" Margin="4,20,0,0" FontFamily="Consolas" FontSize="14" FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left"/>
        <TextBox Height="26" Margin="340,20,0,0" TextWrapping="Wrap" Text="4885" VerticalAlignment="Top" Width="63" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Consolas" FontSize="14" HorizontalAlignment="Left"/>
        <ComboBox Style="{x:Null}" Margin="220,20,0,0" VerticalAlignment="Top" Width="115" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas" Height="26" HorizontalAlignment="Left"/>
        <ComboBox Style="{x:Null}" Margin="467,20,0,0" VerticalAlignment="Top" Width="115" Height="26" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas" HorizontalAlignment="Left"/>
        <TextBox Height="26" Margin="408,20,0,0" TextWrapping="Wrap" Text="1000" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas" Width="54" HorizontalAlignment="Left"/>
        <TextBox Height="26" Margin="587,20,0,0" TextWrapping="Wrap" Text="1234568A-00" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas" HorizontalAlignment="Left" Width="97"/>
        <ComboBox Style="{x:Null}" Margin="105,20,0,0" VerticalAlignment="Top" Height="26" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas" Width="110" HorizontalAlignment="Left"/>
        <Button ToolTip="Cortar selección al Portapapeles" VerticalAlignment="Top" Margin="632,61,0,0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" HorizontalAlignment="Left">
            <Image Source="/IngresoPedidos;component/Resources/save.ico" Width="48" Height="48" VerticalAlignment="Top" HorizontalAlignment="Left"  />
        </Button>
        <Button ToolTip="Agregar a lista personalizada" IsEnabled="False" VerticalAlignment="Top" Margin="575,61,0,0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" HorizontalAlignment="Left">
            <Image Source="/IngresoPedidos;component/Resources/128 (16).ico" Width="48" Height="48" VerticalAlignment="Top" HorizontalAlignment="Left" />
        </Button>

    </Grid>
</Window>

