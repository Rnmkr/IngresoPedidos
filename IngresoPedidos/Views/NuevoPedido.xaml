<Window x:Class="IngresoPedidos.Views.NuevoPedido"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IngresoPedidos.Views"
        mc:Ignorable="d"
        Title="ALTA DE NUEVO PEDIDO" Height="230" Width="785" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">

    <Grid Margin="10">
        <Label Content="PEDIDO" HorizontalAlignment="Left" Margin="0,5,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="120" Height="15" VerticalAlignment="Top"/>
        <Label Content="ARTICULO" HorizontalAlignment="Left" Margin="375,5,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="65"/>
        <Label Content="MODELO" HorizontalAlignment="Left" Margin="125,5,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="120"/>
        <Label Content="PRODUCTO" HorizontalAlignment="Left" Margin="250,5,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="120" Height="15"/>
        <Label Content="CANTIDAD" HorizontalAlignment="Left" Margin="445,5,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="65"/>
        <Label Content="ORIGINAL" HorizontalAlignment="Left" Margin="640,5,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" Width="119"/>
        <Label Content="ESTADO" Margin="0,5,124,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontWeight="Bold" Padding="1" HorizontalAlignment="Right" Width="120"/>
        <TextBox Height="26" TextWrapping="Wrap" VerticalAlignment="Top" Margin="0,25,639,0" FontFamily="Consolas" FontSize="14" FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
        <ComboBox Style="{x:Null}" Margin="125,25,514,0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas" Height="26"/>
        <ComboBox Style="{x:Null}" Margin="250,25,389,0" VerticalAlignment="Top" Height="26" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas"/>
        <TextBox Height="26" Margin="375,25,319,0" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Consolas" FontSize="14"/>
        <TextBox Height="26" Margin="445,25,249,0" TextWrapping="Wrap" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas"/>
        <ComboBox Style="{x:Null}" Margin="515,25,124,0" VerticalAlignment="Top" Height="26" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas"/>
        <TextBox Height="26" Margin="640,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontFamily="Consolas"/>

        <TextBox Margin="0,56,0,0" TextWrapping="Wrap" MaxLength="240" MaxLines="3" Padding="3" FontFamily="Consolas" FontSize="14" Height="65" VerticalAlignment="Top"/>

        <Button ToolTip="Cortar selección al Portapapeles" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}">
            <Image Source="/IngresoPedidos;component/Resources/save.ico" Width="48" Height="48"  />
        </Button>
    </Grid>
</Window>

